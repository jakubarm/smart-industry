{
  "hosting": {
    "public": "build/es6-unbundled",
    "ignore": [
      "**/.*",
      "firebase.json"
    ],
    "rewrites": [
      {
        "source": "**/!(*.*)",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "**/service-worker.js",
        "headers": [
          {
            "key": "Service-Worker-Allowed",
            "value": "/"
          }
        ]
      }
    ]
  },
  "database": {
    "rules": "database.rules.json"
  }
}
